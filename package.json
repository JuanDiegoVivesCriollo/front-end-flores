{
  "name": "front-end-floresydetalles",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build && npm run postbuild",
    "build:production": "NODE_ENV=production next build && npm run postbuild",
    "build:hostinger": "NODE_ENV=production next build && npm run postbuild && npm run deploy:hostinger",
    "build:optimized": "npm run compress:images && npm run build",
    "postbuild": "node scripts/post-build.js",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "compress:images": "node scripts/compress-images.mjs",
    "compress:catalog": "node scripts/compress-catalog-images.js",
    "analyze:bundle": "ANALYZE=true npm run build",
    "deploy:hostinger": "powershell -ExecutionPolicy Bypass -File .\\deploy-hostinger.ps1"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-brands-svg-icons": "^7.0.0",
    "@fortawesome/free-regular-svg-icons": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^3.0.0",
    "@lyracom/embedded-form-glue": "^1.4.4",
    "@types/leaflet": "^1.9.20",
    "framer-motion": "^12.23.12",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.542.0",
    "next": "15.4.6",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-intersection-observer": "^9.16.0",
    "react-leaflet": "^5.0.0",
    "react-scroll": "^1.9.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "postcss": "^8.5.6",
    "sharp": "^0.34.3",
    "tailwindcss": "3.4.10",
    "typescript": "^5"
  }
}
